{
    "name": "gbhorwood/redactem",
    "description": "A json redaction utility",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gbhorwood\\Redactem\\": "src/"
        }
    },
    "authors": [
        {
            "name": "grant horwood",
            "email": "ghorwood@fruitbat.studio"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^10.2",
        "php-mock/php-mock-phpunit": "^2.7",
        "squizlabs/php_codesniffer": "^3.12",
        "slevomat/coding-standard": "^8.16"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
